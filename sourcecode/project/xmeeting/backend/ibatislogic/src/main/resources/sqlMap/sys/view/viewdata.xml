<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap        PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"       "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="sys.view">
 
 
	<select id="queryViewDataGrid" parameterClass="java.util.HashMap" resultClass="java.util.HashMap"  remapResults="true">  
		<![CDATA[   
	              $sqltext$
		]]>
		 
	</select>
	
	
	<select id="queryViewDataGridCount" parameterClass="java.util.HashMap" resultClass="java.lang.Integer" >  
		<![CDATA[   
	              $countsqltext$
		]]>
		 
	</select>
	
	
	

</sqlMap>

