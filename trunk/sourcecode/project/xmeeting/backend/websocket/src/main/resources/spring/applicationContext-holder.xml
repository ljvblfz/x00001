<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd"
	default-autowire="byName" default-lazy-init="true">


	<bean id="springDaoHolder" class="com.broadsoft.xmeeting.springholder.SpringDaoHolder" factory-method="getInstance" scope="singleton"  lazy-init="false">
		<property name="xmPersonnelInfoDao" ref="xmPersonnelInfoDao" />
		<property name="xmMeetingVoteDao" ref="xmMeetingVoteDao" />
		<property name="xmMeetingVoteDetailDao" ref="xmMeetingVoteDetailDao" />
		<property name="xmMeetingMessageDao" ref="xmMeetingMessageDao" />
		<property name="xmMeetingCallDao" ref="xmMeetingCallDao" />
	</bean>




</beans>
